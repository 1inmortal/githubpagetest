{
  "name": "GitHub Secrets Auditor",
  "queries": [
    "security-and-quality",
    "security-extended",
    "python-security-and-quality.qls"
  ],
  "paths": [
    "audit-secrets.py",
    "setup.py",
    "verify-codeql-setup.py",
    "__init__.py",
    "list-project-files.py",
    "Evidencias/**/*.py",
    "assets/**/*.js",
    "assets/**/*.ts",
    "assets/**/*.jsx",
    "assets/**/*.tsx"
  ],
  "paths-ignore": [
    "**/node_modules/**",
    "**/__pycache__/**",
    "**/.venv/**",
    "**/venv/**",
    "**/env/**",
    "**/build/**",
    "**/dist/**",
    "**/*.egg-info/**",
    "**/.venv",
    "**/venv",
    "**/env",
    "**/build",
    "**/dist",
    "**/*.egg-info",
    "**/Lib/site-packages/**",
    "**/site-packages/**",
    "**/Scripts/**",
    "**/bin/**",
    "**/include/**",
    "**/share/**"
  ]
}
