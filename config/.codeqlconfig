{
  "name": "GitHub Secrets Auditor",
  "queries": [
    "security-and-quality",
    "security-extended"
  ],
  "paths-ignore": [
    "**/node_modules/**",
    "**/__pycache__/**",
    "**/.venv/**",
    "**/venv/**",
    "**/env/**",
    "**/build/**",
    "**/dist/**",
    "**/*.egg-info/**",
    "**/Lib/site-packages/**",
    "**/site-packages/**",
    "**/Scripts/**",
    "**/bin/**",
    "**/include/**",
    "**/share/**"
  ]
}
